@model IEnumerable<Lokanta.Models.Event>
@{
    Layout = null;
}
@foreach(var item in Model) { 
<div class="col-md-4">
    <div class="fh5co-event to-animate-2">
        <h3>@item.eventBaslik</h3>
        <span class="fh5co-event-meta">@item.eventTarih</span>
        <p>@item.eventAciklama</p>
        <p><a href="#" class="btn btn-primary btn-outline">Daha Fazla</a></p>
    </div>
</div>
}



